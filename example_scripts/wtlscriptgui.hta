<html>
<head>
<title>Command Line Agruments</title>
<HTA:APPLICATION  ID="objHTA" APPLICATIONNAME="Command Line Arguments" SINGLEINSTANCE="yes">
</head>
<script>
function body_onload() {
    var arg = objHTA.commandLine.substr(objHTA.commandLine.lastIndexOf('"')+2);
    document.write("Arguments: " + arg);
}
</script>
<body>
</head>
<body onload="body_onload()">
</body>
